void BSDOD()
{
    BOOLEAN bl;
    unsigned long responce;
    RtlAdjustPrivilege(19, true, false, &bl);
    NtRaiseHardError(STATUS_ASSERTION_FAILURE, 0, 0, 0, 6, &responce);
    return;
}


